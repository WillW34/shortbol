use <sbol>

@prefix sbol_prefix = <http://sbol_prefix.org/>
@prefix sbol_prefix
# Answer 4
# The sequences of the TetR inverter parts
pTetR_seq is a sbol.DnaSequence("tccctatcagtgatagagattgacatccctatcagtgatagagatactgagcac")


# The genetic parts of the TetR inverter
pTetR is a sbol.Promoter()
(
    sbol.sequence = pTetR_seq
)

#Build components for each of the DNAComponents
pTetR_c is a sbol.Component(pTetR)

#Specify the range for each part
pTetR_loc is a sbol.InlineRange(1,55)

pTetR_sa is a sbol.SequenceAnnotation(pTetR_loc)
(
    sbol.component = pTetR_c
)


tetRInverter is a sbol.DnaComponent()
(
  # include the child components
    sbol.component = pTetR_c

  # absolute positions of child components
    sbol.sequenceAnnotation = pTetR_sa

)

@extension SbolIdentity()
