    hasDNASequence("")
    sequenceAnnotation(PacI, all_or_nothingSequenceAnnotation3Range)
    sequenceAnnotation(all_or_nothing_Pm, all_or_nothingSequenceAnnotationRange)
    sequenceAnnotation(all_or_nothing_PA1_04s, all_or_nothingSequenceAnnotation1Range)
    sequenceAnnotation(SpeI, all_or_nothingSequenceAnnotation2Range)

