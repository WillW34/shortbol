    precedes(all_or_nothing_Pm, all_or_nothing_PA1_04s)
    precedes(all_or_nothing_PA1_04s, SpeI)


)

GFP is a Protein()
pLacI is a Protein()
XylS is a Protein()
coupled_RNA is a RNA()
sRNA is a RNA()

GFP_i is a InstanceOf(GFP)
LacI_i is a InstanceOf(LacI)
pLacI_i is a InstanceOf(pLacI)
coupled_RNA_i is a InstanceOf(coupled_RNA)
XylS_i is a InstanceOf(XylS)
XylSPm_i is a InstanceOf(XylSPm)
sRNA_target_i is a InstanceOf(sRNA_target)
sRNA_i is a InstanceOf(sRNA)
PM_i is a InstanceOf(all_or_nothing_Pm)
PA1_04s_i is a InstanceOf(PA1_04s)
msfGFP_is is a InstanceOf(msfGFP)

all_or_nothing_mod is a ModuleDefinition()
(
    # XYlS produces its protein
    geneticProduction(XylSPm_i, XylS_i)

    # laci produces RNA
    geneticProduction(LacI_i, coupled_RNA_i)

    # GFP (Target) produces RNA
    geneticProduction(msfGFP_is, coupled_RNA_i)

    # PM_RNA produces pLacI
    geneticProduction(coupled_RNA_i, pLacI_i)

    # PM_RNA produces GFP
    geneticProduction(coupled_RNA_i, GFP_i)

    # sRNA produces sRNA
    geneticProduction(sRNA_target_i,sRNA_i)

    # XylS protein stimulates PM
    stimulation(XylSPm_i, PM_i)

    # sRNA represses RNA
    inhibition(sRNA_i, coupled_RNA_i)
    
    # LacI Protein represses Pa1/04s promoter
    inhibition(pLacI_i, PA1_04s_i)
)