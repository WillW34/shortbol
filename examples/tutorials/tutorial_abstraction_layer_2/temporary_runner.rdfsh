use <sbol>

@prefix sbol_prefix = <http://sbol_prefix.org/>
@prefix sbol_prefix

pTetR is a sbol.Promoter()
(
  sbol.hasDNASequence("tccctatcagtgatagagattgacatccctatcagtgatagagatactgagcac")
)


lacI_RBS is a sbol.RBS()
(
    sbol.name =  "lacI_RBS"
    sbol.description =  "RBS for the lacI CDS"
  sbol.hasDNASequence("aa")
)

lacI_CDS is a sbol.CDS()
(
    sbol.name =  "lacI"
    sbol.description =  "LacI protein coding region"
  sbol.hasDNASequence("aaggaggtg")

)


lacI_term is a sbol.Terminator()
(
    sbol.name =  "lacI_term"
    sbol.description =  "Terminator for the lacI CDS"
  sbol.hasDNASequence("tccctatcagtgatagagattgacatccctatcagtgatagagatactgagcac")
)

pTetR_loc is a sbol.InlineRange(1,55,Inline)
lacI_RBS_loc is a sbol.InlineRange(56,68,Inline)
lacI_CDS_loc is a sbol.InlineRange(169,1197,Inline)
lacI_term_loc is a sbol.InlineRange(1197,1240,Inline)


tetRInverter is a sbol.DNAComponent()
(
  
  sbol.precedes(pTetR,lacI_RBS)
  sbol.precedes(lacI_RBS,lacI_CDS)
  sbol.precedes(lacI_CDS,lacI_term)
  sbol.sequenceAnnotation(pTetR,pTetR_loc)
  sbol.sequenceAnnotation(lacI_RBS,lacI_RBS_loc)
  sbol.sequenceAnnotation(lacI_CDS,lacI_CDS_loc)
  sbol.sequenceAnnotation(lacI_term,lacI_term_loc)
)




TetR is a sbol.ProteinComponent()
LacI is a sbol.ProteinComponent()

TetR_inv is a sbol.ProteinComponent()
LacI_inv is a sbol.ProteinComponent()


TetR_inverter is a sbol.ModuleDefinition()
(
    sbol.description =  "TetR inverter"
  sbol.inhibition(TetR,LacI)
)


toggleSwitch is a sbol.Module(TetR_inverter)
(
    sbol.description =  "toggle switch"
  sbol.mapsUseLocal(TetR,TetR_inv)
  sbol.mapsUseLocal(LacI,LacI_inv)
)


LacI_inverter is a sbol.ModuleDefinition()
(
    sbol.description =  "LacI inverter"
  sbol.inhibition(LacI_inv,TetR_inv)
    sbol.module = toggleSwitch
)

@extension SbolIdentity()
